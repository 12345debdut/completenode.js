	<script>
	CallWebAPI();
	function CallWebAPI() {
    //console.log(token);
    var usertoken= localStorage.getItem('token');
    //console.log(usertoken);
    // New XMLHTTPRequest
    var request = new XMLHttpRequest();
    request.open("GET", "http://localhost:3000/api/questions/user", false);
	request.setRequestHeader("Authorization", usertoken);  
	request.send();
	var res1=request.response;
	var res=JSON.parse(res1);
	console.log(res);
	for(i=0;i<res.length;i++){
		var para = document.createElement("P");                      
		var t = document.createTextNode(res[i].textone); 
		para.setAttribute("class","white-text");						    
		para.appendChild(t);                                          
		document.getElementById("questions").appendChild(para); 
		var span = document.createElement("span");
		var t1= document.createTextNode(res[i].texttwo);
		span.appendChild(t1);
		document.getElementById("questions").appendChild(span);
	}
    // view request status
}
	</script>
